{
    "extends": "standard",
    "env": {
        "mongo": true,
        "es6": true,
        "jquery": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "google": true,
        "axios": true,
        "document": false,
        "window": false,
        "console": false
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "indent": [
            2,
            2
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-multiple-empty-lines": "off"

    }
}